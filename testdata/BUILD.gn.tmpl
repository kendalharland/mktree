import("//build/components.gni")
import("//sdk/cts/build/cts.gni")

executable("%(library_name)-bin") {
  output_name = "%(library_name)-test"
  testonly = true
  sources = [ "main.cc" ]
  deps = []
}

group("tests") {
  testonly = true
  deps = [ ":%(library_name)-bin" ]
}